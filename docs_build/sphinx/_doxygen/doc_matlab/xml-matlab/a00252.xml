        <param>
          <type>in</type>
          <declname>t</declname>
        </param>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
<para>Evaluate the invariants of the system of ODEs:</para>
<para><formula id="20">\[
\mathbf{H}( t, \mathbf{x} ) = \mathbf{0}.
\]</formula> </para>
        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
        <location file="ODE/ODEsystem.m" line="80" column="24"/>
      </memberdef>
      <memberdef kind="function" id="a00252_1abe619a3b4e36777c702352b9d0c83efe" prot="public" static="no" const="no" explicit="no" inline="no" virt="virtual">
        <type>function</type>
        <definition>virtual function ODEsystem::JH</definition>
        <argsstring>(in this, in x, in t)</argsstring>
        <name>JH</name>
        <qualifiedname>ODEsystem::JH</qualifiedname>
        <param>
          <type>in</type>
          <declname>this</declname>
        </param>
        <param>
          <type>in</type>
          <declname>x</declname>
        </param>
        <param>
          <type>in</type>
          <declname>t</declname>
        </param>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
<para>Evaluate the Jacobian of the invariants of the system of ODEs:</para>
<para><formula id="21">\[
\partial \mathbf{H}( t, \mathbf{x} ) / \partial \hat{\mathbf{x}}
\]</formula></para>
<para>where <formula id="19">$ \hat{\mathbf{x}} = \left[ \mathbf{x}, \mathbf{x}&apos; \right]^T $</formula> is the concatenation of the unknown functions and their derivatives. </para>
        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
        <location file="ODE/ODEsystem.m" line="94" column="24"/>
