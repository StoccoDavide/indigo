      <memberdef kind="function" id="a00260_1ad6f47719d39790f59cbb1408a3beebb9" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>function</type>
        <definition>function RKimplicit::solveStep</definition>
        <argsstring>(in this, in t_k, in x_k, in d_t)</argsstring>
        <name>solveStep</name>
        <qualifiedname>RKimplicit::solveStep</qualifiedname>
        <param>
          <type>in</type>
          <declname>this</declname>
        </param>
