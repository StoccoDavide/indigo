<Group hide-input="false" labelreference="L757" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">InstallerBuilder:-Build(
  MODULE_NAME,
  'uninstaller' = false,
  'version'     = &quot;1.0&quot;,
  'height'      = 550,
  'width'       = 1000,
  'author'      = &quot;Davide Stocco and Enrico Bertolazzi&quot;,
  'welcome'     = [
     'text'  = &quot;Welcome to the Indigo Maple module installer.&quot;,
     'image' = LOGO
   ],
  'finish'      = [
     'text'  = &quot;The Indigo Maple maodule has been succesfully installed.&quot;, 
     'image' = LOGO
   ],
  'target'      = INSTALL_MLA,
  'license'     = [
     'text'  = LICENSE,
     'image' = LOGO
   ],
  'installation' = [
     'validate' = proc()

        local file, dest, n, r, d, f, limb, ds, RmDir;

        ds   := kernelopts('dirsep');
        file := cat(interface(worksheetdir), ds, &quot;bin&quot;);
        dest := cat(kernelopts(mapledir), ds, &quot;toolbox&quot;, ds, &quot;LULEM&quot;, ds, &quot;lib&quot;, ds);

        ###
        RmDir := proc( dirpath::string, $ )

          local d, dd, ds;

          ds := kernelopts('dirsep');
          if FileTools:-Exists(dirpath) and FileTools:-IsDirectory(dirpath) then
            for d in FileTools:-ListDirectory(dirpath, 'all') do
              if evalb((d &lt;&gt; &quot;.&quot;) and (d &lt;&gt; &quot;..&quot;)) then
                dd := cat(dirpath, ds, d);
                if FileTools:-Exists(dd) then
                  if FileTools:-IsDirectory(dd) then
                    RmDir(dd);
                    # FileTools:-RemoveDirectory(dd, forceremove = true);
                  else
                   FileTools:-Remove(dd);
                  end if;
                end if;
              end if;
            end do;
            FileTools:-RemoveDirectory(dirpath, forceremove = true);
          end if;
          return;
        
        end proc:
        ###
        RmDir(dest);        
        FileTools:-MakeDirectory(dest, recurse = true);
        n  := 1 + length(file);
        for r in FileTools:-Walk(file, 'topdown', 'followlinks') do
          limb := cat(dest, ds, r[dir][n..], ds);
          for d in r[subdirs] do
            ((d) -&gt; `if`(FileTools:-Exists(d), 0, FileTools:-MakeDirectory(d)))(cat(limb, d));
          end do;
          seq(FileTools:-Copy(cat(r[dir], ds, f), cat(limb, f)), f = r[files]) 
        end do;
        return true;
      end proc, 
      'image' = LOGO
   ]
);
</Text-field>
</Input>
</Group>
