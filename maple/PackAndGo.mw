# Utilities for the 'Indigo' module.
module IndigoUtils:

    # Drop the prefix of the input string &lt;str&gt;.
    DropPrefix( str::string, $ ) :: string

    # Find the position of variable &lt;var&gt; in the list &lt;lst&gt;.
    GetPosition( var::{integer, symbol, function}, lst::list, $ ) :: integer

    # Check if the parameter &lt;param&gt;, with name &lt;param_name&gt; is of type 
    # &lt;param_type&gt;.
    CheckParam( param::anything, param_name::string, param_type::anything, 
                where::string, $ )

    # Check if the parameter field &lt;param_field&gt; in table &lt;tab&gt; is of type 
    # &lt;param_type&gt;, if not print and error message &lt;msg&gt;.
    CheckTableField( tab::table, param_field::string, msg::string, 
                     param_type::anything, $ )

    # Check is the parameter name &lt;param_name&gt; in table &lt;tab&gt; is of type 
    # &lt;param_type&gt;.
    CheckTableName( tab::table, param_name::string, param_type::anything, 
                    where::string, $ )

    # Convert a variable &lt;var&gt; into a string. Variables of type 'x[dot](t)' are 
    # converted into 'x_dot(t)'.
    ToString( var::anything, $ ) :: string

    # Convert a variable &lt;var&gt; into a Ruby string. Variables of type 
    # 'x[dot](t)' are converted into 'x_dot(t)'.
    ToRubyString( var::anything, $ ) :: string

    # Convert a variable &lt;var&gt; into multiple strings. Variables of type 
    # 'x[dot](t)' are converted into 'x_dot(t)'.
    ToStringAndSplit( var::anything, $ ) :: string

    # Concatenates input symbols.
    CatSymbol( )

    # Convert variable &lt;var&gt; into a string with prefix &lt;pre&gt; and postfix 
    # &lt;post&gt;. Variables of type 'x[dot](t)' are converted into 'x_dot(t)'.
    ToSymbol( pre::{string, symbol}, var::{string, symbol}, 
              post::{string, symbol}, $ ) :: symbol

    # Convert a variable &lt;var&gt; to a column vector.
    ToVector( var::anything, $ ) :: Vector

    # Combine a list of strings or symbols into one unique symbol.
    JoinSymbols( )

    # Differentiate an expression with respect to a function.
    DoDiff( )

    # Differentiate a scalar expression &lt;fnc&gt; with respect to a list of 
    # functions &lt;lst&gt;.
    DoGradient( fnc::anything, lst::list, $ ) :: anything

    # Differentiate a vector of expressions (gradient) &lt;fnc&gt; with respect to a 
    # list &lt;lst&gt; of functions.
    DoHessian( fnc::anything, lst::list, $ ) :: anything

    # Differentiate a vector of expressions &lt;fnc&gt; with respect to a list &lt;lst&gt; 
    # of functions.
    DoJacobian( fnc::Vector, lst::list, $ ) :: anything

    # Extract the list variables to substitute from a list of parameters 
    # &lt;pars&gt;.
    ExtractSubs( pars::list, $ ) :: list

    # Extract the list of the functions in the expression &lt;expr&gt;, where &lt;onlyD&gt; 
    # is boolean to extract only the functions of the form D(f).
    GetFunctions( expr::anything, onlyD::boolean := false, $ ) :: anything

    # Extract the list of the functions in the expression &lt;expr&gt; that are also 
    # in the list of functions &lt;fncs&gt;.
    GetFunctionsInList( expr::anything, fncs::list, $ ) :: anything

    # Extracts the list of the symbols in the expression &lt;expr&gt;.
    GetSymbols( expr::anything, $ ) :: anything

    # Extracts the function prototype from an expression &lt;expr&gt; of type 'f(x) = 
    # body' or 'f(x)'.
    GetFunctionPrototype( expr::anything, $ ) :: anything

    # Extracts the function name from an expression &lt;expr&gt; of type 'f(x) = 
    # body' or 'f(x)'.
    GetFunctionName( expr::anything, $ ) :: anything

    # Extracts the body from an expression &lt;expr&gt; of type 'f(x) = body' or 
    # 'f(x)'.
    GetFunctionBody( expr::anything, $ ) :: anything

    # Checks if the expression &lt;expr&gt; depends on the list of variables &lt;vars&gt;.
    IsDependentOn( expr::algebraic, vars::list(function), $ ) :: boolean

    # Extracts the set/list of the symbols recursively from the expression 
    # &lt;expr&gt;.
    GetSymbolsRecurr( expr::anything, $ ) :: {list, set}

    # Compute the permutation that sorts a list &lt;var&gt;.
    GetPermSortedList( var::list, $ ) :: list

    # Print a message in color where &lt;fg&gt; is the foreground color, &lt;bg&gt; is the 
