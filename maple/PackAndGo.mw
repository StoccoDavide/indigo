<Output><Text-field style="Line Printed Output" layout="Line Printed Output">
# 'Indigo' module.
object Indigo :: Indigo:

    # Print 'LAST' module information.
    Info( )

    # 'Indigo' module load procedure.
    ModuleLoad( )

    # 'Indigo' module unload procedure.
    ModuleUnload( )

    # Copy the objects &lt;proto&gt; into &lt;self&gt;.
    ModuleCopy( _self::LAST, proto::LAST, $ )

    # Initialize the 'LAST' object with veiling label &lt;label&gt;.
    InitLAST( _self::Indigo, label::{string, symbol} := NULL, $ )

    # Clear the 'LAST' (and 'LEM') object.
    ClearLAST( _self::Indigo, $ )

    # Set the 'LAST' (and 'LEM') object &lt;obj&gt;.
    SetLAST( _self::Indigo, obj::LAST, $ )

    # Get the 'LAST' object.
    GetLAST( _self::Indigo, $ ) :: LAST

    # Set the 'LEM' object &lt;obj&gt;.
    SetLEM( _self::LAST, obj::LEM, $ )

    # Get the 'LEM' object.
    GetLEM( _self::Indigo, $ ) :: LEM

    # Enable the verbose mode.
    EnableVerboseMode( _self::Indigo, $ )

    # Disable the verbose mode.
    DisableVerboseMode( _self::Indigo, $ )

    # Set the veiling symbol as a string &lt;sym&gt;.
    SetVeilingSymbol( _self::Indigo, sym::string, $ )

    # Reset the library.
    Reset( _self::Indigo, label::{string, symbol} := NULL, $ )

    # Get the list of reduction steps.
    GetReductionSteps( _self::Indigo, i::nonnegint := 0, $ ) :: list(table)

    # Get the list of differential equations of the reduced system.
    GetDifferentialEquations( _self::Indigo, $ ) :: list

    # Get the list of hidden constraints of the reduced system.
    GetHiddenConstraints( _self::Indigo, $ ) :: list

    # Get the list of index-1 constraints of the reduced system.
    GetIndexOneConstraints( _self::Indigo, $ ) :: list

    # Get the list of veil arguments substitutions.
    GetVeilArgsSubs( _self::Indigo, rng::range := 1 .. -1, $ )
                   :: list(algebraic = algebraic)

    # Remove the time states from the system.
    RemoveTimeStates( _self::Indigo, 
                      arg::{algebraic, list(algebraic), list(algebraic = 
                        algebraic), algebraic = algebraic}, 
                      $ )
                    :: {algebraic, list(algebraic), list(algebraic = algebraic), algebraic = algebraic}

    # Return the type of the system.
    GetSystemType( _self::Indigo, $ ) :: symbol

    # Build the kernel of a matrix &lt;E&gt;, and return the matrix N such that E*N = 
    # 0 and the rank of E.
    KernelBuild( _self::Indigo, E::Matrix, $ ) :: table

    # Load the matrices from the input DAE system of type &lt;typ&gt;.
    LoadMatrices( _self::Indigo, typ::symbol )

    # Load the matrices from the input DAE system of type &lt;typ&gt;.
    LoadEquations( _self::Indigo, typ::symbol )

    # Reduce the index of the loaded DAE system.
    ReduceIndex( _self::Indigo, $ ) :: boolean

    # Load a 'generic' type system of equations as matrices &lt;E&gt; and &lt;G&gt;. The 
    # list of variables &lt;vars&gt; must be the same of the variables used in the 
    # system of equations.
    LoadMatrices_Linear( _self::Indigo, E::Matrix, G::Matrix, f::Vector, 
                         vars::Vector, $ )

    # Load a 'linear' type system of equations &lt;eqns&gt;. The list of variables 
    # &lt;vars&gt; must be the same of the variables used in the system of equations.
    LoadEquations_Linear( _self::Indigo, eqns::list, vars::list, $ )

    # Reduce the index of the 'linear' type DAE system of equations by one. 
    # Return true if the system of equations has been reduced to index-0 DAE 
    # (ODE), false otherwise.
    ReduceIndexByOne_Linear( _self::Indigo, $ ) :: boolean

    # Reduce the index of the 'linear' type DAE system of equations. Return 
    # true if the system of equations has been reduced to index-0 DAE (ODE), 
    # false otherwise.
    ReduceIndex_Linear( _self::Indigo, $ ) :: boolean

    # Separate algebraic and differential equations from DAE system equations 
    # matrix &lt;E&gt; and differential variables vector &lt;G&gt;. Return the algebraic 
    # equations matrix &lt;Et&gt;, the algebraic variables matrix &lt;Gt&gt;, the 
    # differential equations matrix &lt;A&gt;, and the rank of &lt;E&gt;.
    SeparateMatrices( _self::Indigo, E::Matrix, G::Vector, $ )

    # Load a 'generic' type system of equations as matrices &lt;E&gt; and &lt;G&gt;. The 
    # list of variables &lt;vars&gt; must be the same of the variables used in the 
    # system of equations.
    LoadMatrices_Generic( _self::Indigo, E::Matrix, G::Vector, vars::list, $ )

    # Load a 'generic' type system of equations &lt;eqns&gt;. The list of variables 
    # &lt;vars&gt; must be the same of the variables used in the system of equations.
    LoadEquations_Generic( _self::Indigo, eqns::list, vars::list, $ )

    # Reduce the index of the 'generic' type DAE system of equations by one. 
    # Return true if the system of equations has been reduced to index-0 DAE 
    # (ODE), false otherwise.
    ReduceIndexByOne_Generic( _self::Indigo, $ ) :: boolean

    # Reduce the index of the 'generic' type DAE system of equations. Return 
    # true if the system of equations has been reduced to index-0 DAE (ODE), 
    # false otherwise.
    ReduceIndex_Generic( _self::Indogo, $ ) :: boolean

    # Load a 'mbd3' type system of equations with mass matrix &lt;Mass&gt;, 
    # constraint vector &lt;Phi&gt;, external force vector &lt;f&gt;, the position 
    # variables &lt;q_vars&gt;, the velocity variables &lt;v_vars&gt; and the Lagrange 
    # multipliers &lt;l_vars&gt;.
    LoadMatrices_Mbd3( _self::Indigo, Mass::Matrix, Phi::Vector, f::Vector, 
                       q_vars::list, v_vars::list, l_vars::list, $ )

    # Load a 'Mdb' type system of equations &lt;eqns&gt; with the position variables 
    # &lt;q_vars&gt;, the velocity variables &lt;v_vars&gt; and the Lagrange multipliers 
    # &lt;l_vars&gt;.
    LoadEquations_Mbd3( _self::Indigo, eqns::list, q_vars::list, v_vars::list, 
                        l_vars::list, $ )

    # Reduce the index of the multibody DAE system of equations. Return true if 
    # the system of equations has been reduced to index-0 DAE, false otherwise.
    ReduceIndex_Mbd3( _self::Indigo, 
                      { baumgarte::boolean := false, 
                        jacobians::boolean := false }, $ ) :: boolean

</Text-field>
