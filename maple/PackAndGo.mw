<Group labelreference="L256751" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Describe(Indigo):</Text-field>
</Input>
<Output><Text-field style="Line Printed Output" layout="Line Printed Output">
# 'Indigo' module
module Indigo:

    # Reset the library.
    Reset( $ )

    # Set the warning level as &lt;lvl&gt;.
    SetWarningLevel( lvl::{nonnegint}, $ ) :: {nothing}

    # Set the veiling symbol as a string &lt;sym&gt;.
    SetVeilingSymbol( sym::{string}, $ ) :: {nothing}

    # Build the kernel of a matrix &lt;E&gt;, and return the matrix N such that E*N = 
    # 0 and the rank of E.
    KernelBuild( E::{Matrix}, $ )

    # Load the matrices from the input DAE system of type &lt;typ&gt;.
    LoadMatrices( typ::{symbol} ) :: {nothing}

    # Load the matrices from the input DAE system of type &lt;typ&gt;.
    LoadEquations( typ::{symbol} ) :: {nothing}

    # Reduce the index of the loaded DAE system.
    ReduceIndex( $ )

    # Load a 'Generic' type system of equations as matrices &lt;E&gt; and &lt;G&gt;. The 
    # list of variables &lt;vars&gt; must be the same of the variables used in the 
    # system of equations.
    LoadMatrices_Linear( E::{Matrix}, G::{Matrix}, f::{Vector}, 
                         vars::{Vector}, $ ) :: {nothing}

    # Load a 'Linear' type system of equations &lt;eqns&gt;. The list of variables 
    # &lt;vars&gt; must be the same of the variables used in the system of equations.
    LoadEquations_Linear( eqns::{list}, vars::{list}, $ ) :: {nothing}

    ReduceIndexByOne_Linear( $ )

    # Reduce the index of the 'Linear' type DAE system of equations. Return 
    # true if the system of equations has been reduced to index-0 DAE, false 
    # otherwise.
    ReduceIndex_Linear( $ )

    # Load a 'Generic' type system of equations as matrices &lt;E&gt; and &lt;G&gt;. The 
    # list of variables &lt;vars&gt; must be the same of the variables used in the 
    # system of equations.
    LoadMatrices_Generic( E::{Matrix}, G::{Vector}, vars::{list}, $ )
                        :: {nothing}

    # Load a 'Generic' type system of equations &lt;eqns&gt;. The list of variables 
    # &lt;vars&gt; must be the same of the variables used in the system of equations.
    LoadEquations_Generic( eqns::{list}, vars::{list}, $ ) :: {nothing}

    # Reduce the index of the 'Linear' type DAE system of equations by one. 
    # Return true if the system of equations has been reduced to index-0 DAE, 
    # false otherwise.
    ReduceIndexByOne_Generic( $ )

    # Reduce the index of the 'Generic' type DAE system of equations. Return 
    # true if the system of equations has been reduced to index-0 DAE, false 
    # otherwise.
    ReduceIndex_Generic( $ )

    # Load a 'Mbd3' type system of equations with mass matrix &lt;Mass&gt;, 
    # constraint vector &lt;Phi&gt;, external force vector &lt;f&gt;, the position 
    # variables &lt;q_vars&gt;, the velocity variables &lt;v_vars&gt; and the Lagrange 
    # multipliers &lt;l_vars&gt;.
    LoadMatrices_Mbd3( Mass::{Matrix}, Phi::{Vector}, f::{Vector}, 
                       q_vars::{list}, v_vars::{list}, l_vars::{list}, $ )
                     :: {nothing}

    # Load a 'Mdb' type system of equations &lt;eqns&gt; with the position variables 
    # &lt;q_vars&gt;, the velocity variables &lt;v_vars&gt; and the Lagrange multipliers 
    # &lt;l_vars&gt;.
    LoadEquations_Mbd3( eqns::{list}, q_vars::{list}, v_vars::{list}, 
                        l_vars::{list}, $ ) :: {nothing}

    # Reduce the index of the multibody DAE system of equations. Return true if 
    # the system of equations has been reduced to index-0 DAE, false otherwise.
    ReduceIndex_Mbd3( { baumgarte::{boolean} := false, 
                        jacobians::{boolean} := false }, $ ) :: {boolean}

    SystemType::symbol = Empty

    ReductionSteps::list = []

</Text-field>
</Output>
